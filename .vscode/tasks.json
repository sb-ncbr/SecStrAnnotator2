{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/SecStrAnnotator2.csproj"
            ],
            "dependsOn": "write_buildtime",
            "problemMatcher": "$msCompile"
        },
        {
            "label": "write_buildtime",
            "command": "date --utc +'%F %T' > ${workspaceFolder}/buildtime",
            "type": "shell"
        }
    ]
}